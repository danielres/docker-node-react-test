{
	"name": "2017-05-create-react-app-and-backend-behind-proxy",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"babel-register": "^6.24.1",
		"concurrently": "^3.5.0",
		"dotenv": "^4.0.0",
		"express": "^4.15.3",
		"express-http-proxy": "^1.0.6",
		"history": "^4.6.3",
		"knex": "^0.13.0",
		"lean-redux": "^3.3.1",
		"pg": "^7.0.1",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-icons": "^2.2.5",
		"react-redux": "^5.0.5",
		"react-router": "^4.1.2",
		"react-router-redux": "next",
		"redux": "^3.7.2",
		"styled-components": "^2.1.1"
	},
	"devDependencies": {
		"eslint": "^4.3.0",
		"eslint-config-prettier": "^2.3.0",
		"eslint-plugin-prettier": "^2.1.2",
		"nodemon": "^1.11.0",
		"prettier": "^1.5.3",
		"react-scripts": "^1.0.10"
	},
	"scripts": {
		"docker:dev": "docker-compose -f docker-compose.dev.yml up",
		"docker:migrate": "docker-compose -f docker-compose.dev.yml run web npm run migrate",
		"docker:migrate:make": "docker-compose -f docker-compose.dev.yml run web npm run migrate:make",
		"docker:run": "docker-compose -f docker-compose.dev.yml run web npm run",
		"docker:rollback": "docker-compose -f docker-compose.dev.yml run web npm run rollback",
		"dev": "concurrently 'npm run backend:dev' 'BROWSER=none npm run frontend:dev'",
		"backend:dev": "nodemon --inspect=5858 -r ./babelhook.js ./server.js",
		"frontend:dev": "react-scripts start",
		"backend:production": "ASSETS_MODE=static node -r ./babelhook.js ./server.js",
		"build": "react-scripts build",
		"migrate": "knex migrate:latest",
		"migrate:make": "knex migrate:make",
		"rollback": "knex migrate:rollback",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"lint": "eslint ."
	},
	"proxy": "http://localhost:3001"
}